package Exercicio2;

public class Exercicio2 {
    public static void main(String[] args) {
        //Use 8 print statements to print a smiley face.
        //Your art will rely on only a single character, besides space, such as X or #.
        System.out.println("  ####  ");
        System.out.println(" #    # ");
        System.out.println("# #  # #");
        System.out.println("#      #");
        System.out.println("# #  # #");
        System.out.println("#  ##  #");
        System.out.println(" #    # ");
        System.out.println("  ####  ");
    }
}
